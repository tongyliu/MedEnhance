<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mhacks.tong.medchance" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/med_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".DiseaseSelectionActivity"
            android:label="@string/title_activity_disease" >

        </activity>
        <activity
            android:name=".SymptomsActivity"
            android:label="@string/title_activity_symptoms" >
        </activity>
        <activity
            android:name=".ChanceActivity"
            android:label="@string/title_activity_chance" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>

